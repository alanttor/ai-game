\n                llm: { question_type: \"ä¸“ä¸šé—®ç­”\", sub_category: \"å”®åè§£ç­”\", confidence: 0.68 },\n                rule: { question_type: \"ä¸“ä¸šé—®ç­”\", sub_category: \"å”®åè§£ç­”\" },\n                label_status: \"pending\",\n                final_label: null\n            }\n        ];\n\n        let currentSample = null;\n\n        function hasConflict(sample) {\n            return sample.llm.question_type !== sample.rule.question_type || \n                   sample.llm.sub_category !== sample.rule.sub_category;\n        }\n\n        function renderSampleList(filter = 'all') {\n            const container = document.getElementById('sampleListContainer');\n            let filtered = samples;\n            \n            if (filter === 'pending') filtered = samples.filter(s => s.label_status === 'pending');\n            else if (filter === 'conflict') filtered = samples.filter(s => hasConflict(s));\n            else if (filter === 'labeled') filtered = samples.filter(s => s.label_status === 'labeled');\n            \n            container.innerHTML = filtered.map(sample => `\n                <div class=\"sample-item ${sample.label_status === 'labeled' ? 'labeled' : ''} ${currentSample?.id === sample.id ? 'active' : ''}\" \n                     onclick=\"selectSample(${sample.id})\">\n                    <div class=\"sample-text\">${sample.text}</div>\n                    <div class=\"sample-meta\">\n                        <span>ç½®ä¿¡åº¦: ${(sample.llm.confidence * 100).toFixed(0)}%</span>\n                        ${hasConflict(sample) ? '<span class=\"sample-badge badge-conflict\">æœ‰å†²çª</span>' : ''}\n                        ${sample.llm.confidence < 0.8 ? '<span class=\"sample-badge badge-low-conf\">ä½ç½®ä¿¡åº¦</span>' : ''}\n                        ${sample.label_status === 'labeled' ? '<span class=\"sample-badge badge-labeled\">å·²æ ‡æ³¨</span>' : ''}\n                    </div>\n                </div>\n            `).join('');\n            \n            updateStats();\n        }\n\n        function updateStats() {\n            document.getElementById('totalCount').textContent = samples.length;\n            document.getElementById('pendingCount').textContent = samples.filter(s => s.label_status === 'pending').length;\n            document.getElementById('labeledCount').textContent = samples.filter(s => s.label_status === 'labeled').length;\n            document.getElementById('conflictCount').textContent = samples.filter(s => hasConflict(s)).length;\n        }\n\n        function selectSample(id) {\n            currentSample = samples.find(s => s.id === id);\n            renderSampleList(document.getElementById('filterSelect').value);\n            renderAnnotationPanel();\n        }\n\n        function renderAnnotationPanel() {\n            if (!currentSample) return;\n            \n            const s = currentSample;\n            const conflict = hasConflict(s);\n            \n            document.getElementById('annotationContent').innerHTML = `\n                <div class=\"panel-title\">æ ‡æ³¨æ ·æœ¬ #${s.id}</div>\n                \n                <div class=\"question-display\">\n                    <div class=\"question-label\">ç”¨æˆ·é—®é¢˜</div>\n                    <div class=\"question-text\">${s.text}</div>\n                </div>\n                \n                <div class=\"predictions\">\n                    <div class=\"prediction-card llm\">\n                        <div class=\"prediction-title llm\">ğŸ¤– LLMé¢„æµ‹</div>\n                        <div class=\"prediction-row\">\n                            <span class=\"prediction-key\">é—®é¢˜ç±»å‹</span>\n                            <span class=\"prediction-value\">${s.llm.question_type}</span>\n                        </div>\n                        <div class=\"prediction-row\">\n                            <span class=\"prediction-key\">å­åˆ†ç±»</span>\n                            <span class=\"prediction-value\">${s.llm.sub_category || '-'}</span>\n                        </div>\n                        <div class=\"prediction-row\">\n                            <span class=\"prediction-key\">ç½®ä¿¡åº¦</span>\n                            <span class=\"prediction-value\">${(s.llm.confidence * 100).toFixed(1)}%</span>\n                        </div>\n                    </div>\n                    <div class=\"prediction-card rule\">\n                        <div class=\"prediction-title rule\">ğŸ“ è§„åˆ™é¢„æµ‹</div>\n                        <div class=\"prediction-row\">\n                            <span class=\"prediction-key\">é—®é¢˜ç±»å‹</span>\n                            <span class=\"prediction-value\">${s.rule.question_type}</span>\n                        </div>\n                        <div class=\"prediction-row\">\n                            <span class=\"prediction-key\">å­åˆ†ç±»</span>\n                            <span class=\"prediction-value\">${s.rule.sub_category || '-'}</span>\n                        </div>\n                        <div class=\"prediction-row\">\n                            <span class=\"prediction-key\">åŒ¹é…è§„åˆ™</span>\n                            <span class=\"prediction-value\">-</span>\n                        </div>\n                    </div>\n                </div>\n                \n                ${conflict ? '<div style=\"background:#fef3c7;padding:12px;border-radius:8px;margin-bottom:20px;color:#92400e;font-size:14px;\">âš ï¸ LLMä¸è§„åˆ™é¢„æµ‹ç»“æœä¸ä¸€è‡´ï¼Œè¯·ä»”ç»†åˆ¤æ–­</div>' : ''}\n                \n                <div class=\"annotation-section\">\n                    <div class=\"section-title\">é€‰æ‹©æ­£ç¡®çš„é—®é¢˜ç±»å‹</div>\n                    <div class=\"label-options\" id=\"typeOptions\">\n                        <label class=\"label-option\" onclick=\"selectType('é—²èŠ')\">\n                            <input type=\"radio\" name=\"questionType\" value=\"é—²èŠ\">\n                            <div class=\"label-radio\"></div>\n                            <div class=\"label-content\">\n                                <div class=\"label-title\">é—²èŠ</div>\n                                <div class=\"label-desc\">é—®å€™ã€é—²èŠã€æ— æ„ä¹‰è¾“å…¥</div>\n                            </div>\n                        </label>\n                        <label class=\"label-option\" onclick=\"selectType('æ•æ„Ÿ')\">\n                            <input type=\"radio\" name=\"questionType\" value=\"æ•æ„Ÿ\">\n                            <div class=\"label-radio\"></div>\n                            <div class=\"label-content\">\n                                <div class=\"label-title\">æ•æ„Ÿ</div>\n                                <div class=\"label-desc\">è¿è§„ã€æ”»å‡»ã€æŠ•è¯‰ç­‰æ•æ„Ÿè¯é¢˜</div>\n                            </div>\n                        </label>\n                        <label class=\"label-option\" onclick=\"selectType('ä¸“ä¸šé—®ç­”')\">\n                            <input type=\"radio\" name=\"questionType\" value=\"ä¸“ä¸šé—®ç­”\">\n                            <div class=\"label-radio\"></div>\n                            <div class=\"label-content\">\n                                <div class=\"label-title\">ä¸“ä¸šé—®ç­”</div>\n                                <div class=\"label-desc\">æŠ€æœ¯ç›¸å…³é—®é¢˜ï¼ˆå”®å‰/å”®åï¼‰</div>\n                            </div>\n                        </label>\n                    </div>\n                    \n                    <div class=\"sub-category-section\" id=\"subCategorySection\" style=\"display:none;\">\n                        <div class=\"section-title\">é€‰æ‹©å­åˆ†ç±»</div>\n                        <div class=\"sub-options\">\n                            <div class=\"sub-option\" onclick=\"selectSubCategory('å”®åè§£ç­”')\">å”®åè§£ç­”</div>\n                            <div class=\"sub-option\" onclick=\"selectSubCategory('å”®å‰äº§å“æ¨è')\">å”®å‰äº§å“æ¨è</div>\n                        </div>\n                    </div>\n                </div>\n                \n                <div class=\"action-buttons\">\n                    <button class=\"btn btn-primary\" onclick=\"submitLabel()\">âœ“ æäº¤æ ‡æ³¨</button>\n                    <button class=\"btn btn-secondary\" onclick=\"useLLM()\">é‡‡ç”¨LLMç»“æœ</button>\n                    <button class=\"btn btn-secondary\" onclick=\"useRule()\">é‡‡ç”¨è§„åˆ™ç»“æœ</button>\n                    <button class=\"btn btn-skip\" onclick=\"skipSample()\">è·³è¿‡</button>\n                </div>\n            `;\n        }\n\n        let selectedType = null;\n        let selectedSubCategory = null;\n\n        function selectType(type) {\n            selectedType = type;\n            document.querySelectorAll('#typeOptions .label-option').forEach(el => el.classList.remove('selected'));\n            event.currentTarget.classList.add('selected');\n            \n            const subSection = document.getElementById('subCategorySection');\n            if (type === 'ä¸“ä¸šé—®ç­”') {\n                subSection.style.display = 'block';\n            } else {\n                subSection.style.display = 'none';\n                selectedSubCategory = null;\n            }\n        }\n\n        function selectSubCategory(sub) {\n            selectedSubCategory = sub;\n            document.querySelectorAll('.sub-option').forEach(el => el.classList.remove('selected'));\n            event.currentTarget.classList.add('selected');\n        }\n\n        function submitLabel() {\n            if (!selectedType) {\n                alert('è¯·é€‰æ‹©é—®é¢˜ç±»å‹');\n                return;\n            }\n            if (selectedType === 'ä¸“ä¸šé—®ç­”' && !selectedSubCategory) {\n                alert('è¯·é€‰æ‹©å­åˆ†ç±»');\n                return;\n            }\n            \n            currentSample.label_status = 'labeled';\n            currentSample.final_label = {\n                question_type: selectedType,\n                sub_category: selectedSubCategory || '',\n                source: 'human'\n            };\n            \n            alert('æ ‡æ³¨å·²ä¿å­˜ï¼');\n            goToNext();\n        }\n\n        function useLLM() {\n            currentSample.label_status = 'labeled';\n            currentSample.final_label = { ...currentSample.llm, source: 'llm' };\n            alert('å·²é‡‡ç”¨LLMç»“æœ');\n            goToNext();\n        }\n\n        function useRule() {\n            currentSample.label_status = 'labeled';\n            currentSample.final_label = { ...currentSample.rule, source: 'rule' };\n            alert('å·²é‡‡ç”¨è§„åˆ™ç»“æœ');\n            goToNext();\n        }\n\n        function skipSample() {\n            goToNext();\n        }\n\n        function goToNext() {\n            const pending = samples.filter(s => s.label_status === 'pending');\n            if (pending.length > 0) {\n                selectSample(pending[0].id);\n            } else {\n                currentSample = null;\n                renderSampleList();\n                document.getElementById('annotationContent').innerHTML = `\n                    <div class=\"empty-state\">\n                        <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                            <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z\" />\n                        </svg>\n                        <p>ğŸ‰ æ‰€æœ‰æ ·æœ¬å·²æ ‡æ³¨å®Œæˆï¼</p>\n                    </div>\n                `;\n            }\n            selectedType = null;\n            selectedSubCategory = null;\n        }\n\n        // åˆå§‹åŒ–\n        document.getElementById('filterSelect').addEventListener('change', (e) => {\n            renderSampleList(e.target.value);\n        });\n        \n        renderSampleList();\n    </script>\n</body>\n</html>",
